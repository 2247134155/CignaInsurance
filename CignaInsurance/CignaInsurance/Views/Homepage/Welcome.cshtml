@model IEnumerable<CignaInsurance.Models.Customer>

@{
    ViewBag.Title = "Welcome";
}

<h2>Welcome to our Project,</h2>
<br />
@foreach (var customer in Model)
{
    @Html.DisplayFor(m => customer.CustomerID); <br />
    @Html.ActionLink(customer.Firstname,"Redirects", "Homepage", new { id = customer.CustomerID}, null) <br />
}



